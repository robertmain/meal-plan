{
  "name": "meal-planner-api",
  "version": "0.0.1",
  "description": "Meal planning API",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "nest build --webpack --webpackPath webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/server.js",
    "lint": "eslint -c .eslintrc.js src/**/*.ts test/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "orm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.6.5",
    "@nestjs/platform-express": "^6.10.13",
    "@nestjs/swagger": "^4.7.10",
    "@nestjs/typeorm": "^6.0.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.16"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "jest": "^24.8.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^6.0.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typemoq": "^2.1.0",
    "typescript": "^3.7.4",
    "webpack": "^5.19.0",
    "webpack-command": "^0.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "verbose": true
  }
}
