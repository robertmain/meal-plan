{
  "name": "meal-planner-api",
  "version": "0.0.1",
  "description": "Meal planning API",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc --project tsconfig.build.json; webpack",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/server/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/server.js",
    "lint": "eslint src/**/*.{ts,vue}",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "orm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.11",
    "@nestjs/core": "^7.6.11",
    "@nestjs/platform-express": "^7.6.11",
    "@nestjs/swagger": "^4.7.10",
    "@nestjs/typeorm": "^6.0.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.13.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@typescript-eslint/typescript-estree": "^4.15.0",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-eslint": "^4.5.10",
    "@vue/cli-plugin-router": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-plugin-unit-jest": "^4.5.10",
    "@vue/cli-plugin-vuex": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.1.3",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.2",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-vue": "^7.5.0",
    "html-webpack-plugin": "^5.0.0",
    "jest": "^24.8.0",
    "node-sass": "^5.0.0",
    "nodemon": "^1.18.9",
    "postcss": "^8.2.5",
    "prettier": "^1.15.3",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "supertest": "^3.4.1",
    "ts-jest": "^26.5.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typemoq": "^2.1.0",
    "typescript": "^3.9.7",
    "vue-eslint-parser": "^7.4.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "verbose": true
  }
}
